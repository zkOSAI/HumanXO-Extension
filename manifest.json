{
  "manifest_version": 3,
  "name": "ZKOS",
  "description": "Leverage your unused internet bandwidth!",
  "homepage_url": "https://zkos.ai/",
  "key": "",
  "action": {
    "default_popup": "login/login.html",
    "default_icon": {
      "32": "/images/icon-32.png"
    }
  },
  "icons": {
    "128": "/images/icon-32.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "activeTab",
    "storage",
    "background",
    "fetch",
    "clipboardWrite",
    "scripting"
  ],
  "web_accessible_resources": [
    {
      "matches": [],
      "resources": [
        "contentStyle.css",
        "icon-128.png",
        "icon-32.png"
      ],
      "use_dynamic_url": false
    }
  ],
  "externally_connectable": {
    "matches": [
      "http://127.0.0.1/*",
      "http://localhost/*",
      "*://*.zkos.ai/*"
    ]
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "version": "0.2.2"
}